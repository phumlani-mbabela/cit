<?xml version="1.0" encoding="utf-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<navigation-rule>

		<from-view-id>/captureTender.xhtml</from-view-id>
		
		<navigation-case>
			<from-action>#{tenderManagedBean.create}</from-action>
			<from-outcome>detailTender</from-outcome>
			<to-view-id>/detailTender.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>
	
	<navigation-rule>

		<from-view-id>/viewTender.xhtml</from-view-id>
		
		<navigation-case>
			<from-outcome>deleted</from-outcome>
			<to-view-id>/tenders.xhtml</to-view-id>
		</navigation-case>

	   <navigation-case>
	      <from-action>#{tendersManagedBean.paginate}</from-action>
	      <from-outcome>viewTender</from-outcome>
	      <to-view-id>/viewTender.xhtml</to-view-id>
	   </navigation-case>
	   
	   <navigation-case>
	      <from-action>#{tendersManagedBean.paginate}</from-action>
	      <from-outcome>tenders</from-outcome>
	      <to-view-id>/tenders.xhtml</to-view-id>
	   </navigation-case>

		<navigation-case>
			<from-outcome>searchTenders</from-outcome>
			<to-view-id>/tenders.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>
	
	
	
	<navigation-rule>

		<from-view-id>/*.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>
	
	<navigation-rule>

		<from-view-id>/profile.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>

</faces-config>